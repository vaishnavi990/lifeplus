        <section class="hospital-section" id="hospital">
            <div class="card-body">
                <h3>Find Ventilator availability</h3>
                <div class="row float-right mb-2">
                  <div class="col-md-12">
                    <div class="input-group">
                      <input type="text" class="form-control form-control-lg" id="myInput" onkeyup="Search()" placeholder="Type your keywords here">
                      <div class="input-group-append">
                          <button type="button" class="btn btn-lg btn-default">
                              <i class="fa fa-search"></i>
                          </button>
                      </div>
                    </div>
                  </div>
                </div>
                <table class="table table-striped table-head-fixed text-nowrap table-hover table-bordered" id="myTable">
                  <thead>
                    <tr>
                      <th style="width: 5%" >Hospital ID</th>
                      <th style="width: 10%" >Ventilator ID</th>
                      <th style="width: 10%" >Status</th>
                      <th style="width: 10%" >Name</th>
                    </tr>
                  </thead>
                  <tbody>
                    <%record.forEach(function(row){%>
                    <tr>
                    <td><%=row.hId%></td>
                    <td><%=row.ventilatorId%></td>
                    <td><%=row.status%></td>
                    <td><%=row.name%></td>
                    
                    </tr>
                    <%}) %>
                </tbody>
                </table>
              </div>
        </section>
